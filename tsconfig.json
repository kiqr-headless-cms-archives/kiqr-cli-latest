{
  "exclude": ["templates"],
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "rootDir": ".",
    "baseUrl": "src",
    "lib": ["ES2019"],
    "target": "ES2019",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "allowJs": false,
    "module": "node16",
    "moduleResolution": "node16",
    "strict": false,
    "paths": {
      "~commands/*": ["commands/*"],
      "~errors/*": ["errors/*"],
      "~lib/*": ["lib/*"],
      "~utils/*": ["utils/*"],
      "~services/*": ["services/*"],
      "~prompts/*": ["prompts/*"],
      "~types/*": ["types/*"],
      "~apis/*": ["apis/*"],
    }
  }
}